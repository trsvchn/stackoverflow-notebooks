    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# generate some data\n",
    "size = 25\n",
    "x = np.linspace(0, 1, size)\n",
    "y = np.exp(x)\n",
    "# \n",
    "counts = np.arange(1, 100, 100 // size)\n",
    "\n",
    "scatter = plt.scatter(x, y, c=counts, cmap='jet')\n",
    "plt.colorbar(scatter)"
   ]
  }
